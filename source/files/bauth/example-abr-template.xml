<?xml version="1.0" encoding="iso-8859-1"?>

<MIRCdocument display="page" background="light">

 <title>Example ABR Template</title>

 <author>
  <name/>
  <affiliation/>
  <contact/>
 </author>

 <abstract><p>None.</p></abstract>

 <section heading="Exam">
 	<p>
 		<ul>
			<checkbox name="exam1">Oral</checkbox>
			<checkbox name="exam2">MOC</checkbox>
			<checkbox name="exam3">MOC Subspecialty</checkbox>
			<checkbox name="exam4">Core</checkbox>
			<checkbox name="exam5">Certifying</checkbox>
			<checkbox name="exam6">Written</checkbox>
		</ul>
 	</p>
 </section>

 <section heading="Category/Subspecialty">
 	<p>
 		<select name="cat">
 			<option>GU</option>
 			<option>NU</option>
 			<option>GI</option>
 			<option>CT</option>
 			<option>VIR</option>
 			<option>Etc.</option>
 		</select>
 	</p>
 </section>

 <section heading="History">
 	<textblock name="hist"/>
 </section>

 <section heading="Modality">
 	<p>
 		<ul>
			<checkbox name="modal1">Radiography</checkbox>
			<checkbox name="modal2">Fluoroscopy</checkbox>
			<checkbox name="modal3">CT</checkbox>
			<checkbox name="modal4">MRI</checkbox>
			<checkbox name="modal5">NM/Scintigraphy</checkbox>
			<checkbox name="modal6">Intervention Rx/Dx</checkbox>
			<checkbox name="modal7">Angiography</checkbox>
			<checkbox name="modal8">US</checkbox>
		</ul>
 	</p>
 </section>

 <section heading="Anatomy">
 	<textblock name="anat"/>
 </section>

 <section heading="Pathology">
 	<textblock name="path"/>
 </section>

 <section heading="Primary Content">
 	<p>
 		<ul>
			<checkbox name="pc1">Anatomy</checkbox>
			<checkbox name="pc3">Pathology</checkbox>
			<checkbox name="pc4">Development</checkbox>
			<checkbox name="pc5">Physiology</checkbox>
			<checkbox name="pc6">Contrast/Drugs</checkbox>
			<checkbox name="pc7">Therapy</checkbox>
			<checkbox name="pc8">Indication/Appropriateness</checkbox>
			<checkbox name="pc9">Clinical</checkbox>
			<checkbox name="pc10">Technique/Instrumentation</checkbox>
			<checkbox name="pc11">Quality Assurance</checkbox>
			<checkbox name="pc12">Emergency/Trauma</checkbox>
 		</ul>
 	</p>
 </section>

 <section heading="Findings or Pattern">
 	<p>
 		<ul>
			<checkbox name="pat1">pattern 1</checkbox>
			<checkbox name="pat3">pattern 2</checkbox>
			<checkbox name="pat4">pattern 3</checkbox>
			<checkbox name="pat5">pattern 4</checkbox>
			<checkbox name="pat6">pattern 5</checkbox>
			<checkbox name="pat7">pattern 6</checkbox>
		</ul>
 	</p>
 </section>

 <section heading="Difficulty">
 	<p>
 		<select name="diff">
 			<option>Primary</option>
 			<option>Intermediate</option>
 			<option>Advanced</option>
 		</select>
 	</p>
 </section>

 <section heading="Management">
 	<textblock name="mgt"/>
 </section>

 <section heading="Image Quality">
 	<p>
 		<select name="qual">
 			<option>Artifact</option>
 			<option>Noise</option>
 			<option>Physics Applicability</option>
 		</select>
 	</p>
 </section>

 <section heading="Diagnosis">
 	<textblock name="diag"/>
 </section>

 <section heading="Notes">
 </section>

 <section heading="Files">
  <p><metadata-refs/></p>
 </section>

 <image-section heading="Images">
 <insert-megasave width="512"/>
 </image-section>

 <authorization>
  <owner/>
  <read/>
  <update/>
  <export/>
 </authorization>

 <publication-date/>

</MIRCdocument>
